{
  "name": "Inteligir",
  "description": "Making open source more accessible for everyone",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inteligir/inteligir-platform"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "encrypt-env": "openssl aes-256-cbc -a -salt -in .env -pass file:encrypted/.inteligir_dev.key -out encrypted/env.enc",
    "decrypt-env": "openssl aes-256-cbc -d -a -salt -in encrypted/env.enc -pass file:encrypted/.inteligir_dev.key -out .env",
    "setup": "npm run decrypt-env && npm install",
    "dev": "nodemon -r dotenv/config server",
    "build": "next build client",
    "start": "nf start",
    "deploy": "node -r dotenv/config tools/deploy",
    "analyze": "source-map-explorer client/build/static/js/main.*",
    "test": "jest",
    "heroku-postbuild": "npm run build"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "./server",
    "@client": "./client"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup.js"
  },
  "dependencies": {
    "adorable-avatars": "0.2.1",
    "aws-sdk": "2.380.0",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "boom": "7.3.0",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.5",
    "evergreen-ui": "4.8.0",
    "express": "4.16.4",
    "express-async-errors": "3.1.1",
    "helmet": "3.15.0",
    "lodash": "4.17.11",
    "migrate-mongoose": "3.2.2",
    "module-alias": "2.1.0",
    "mongoose": "5.4.0",
    "next": "7.0.2",
    "next-redux-wrapper": "2.1.0",
    "nprogress": "0.2.0",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "polished": "2.3.1",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "redis": "2.8.0",
    "redux": "4.0.1",
    "redux-form": "8.0.0",
    "redux-thunk": "2.3.0",
    "scrollama": "1.4.4",
    "styled-components": "4.1.3",
    "uuid": "3.3.2",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "dotenv": "6.2.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.88.0",
    "foreman": "3.0.1",
    "husky": "1.2.1",
    "inquirer": "6.2.1",
    "jest": "23.6.0",
    "nodemon": "1.18.8",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "puppeteer": "1.11.0",
    "source-map-explorer": "1.6.0"
  }
}
