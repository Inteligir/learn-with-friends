{
  "name": "inteligir",
  "description": "Making open source accessible for everyone",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inteligir/inteligir-platform"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "setup": "npm i && node tools/generate-env && npm run deploy:schema",
    "dev": "env-cmd ./config/local.env nodemon server/index.js --ext js,graphql",
    "build": "env-cmd ./config/local.env next build client",
    "deploy": "env-cmd ./config/local.env node tools/deploy",
    "deploy:schema": "env-cmd ./config/local.env prisma deploy",
    "test": "env-cmd ./config/test.env jest --watch --runInBand",
    "storybook": "start-storybook -p 5010 -s ./client",
    "create:components": "node ./tools/components/create-components.js",
    "heroku-postbuild": "npm run build"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "./server",
    "@client": "./client"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup.js"
  },
  "dependencies": {
    "adorable-avatars": "0.2.2",
    "apollo-boost": "0.1.28",
    "aws-sdk": "2.407.0",
    "bcrypt": "3.0.4",
    "boom": "7.3.0",
    "env-cmd": "8.0.2",
    "express-async-errors": "3.1.1",
    "graphql-yoga": "1.17.4",
    "graphql-shield": "5.3.0",
    "helmet": "3.15.1",
    "isomorphic-unfetch": "3.0.0",
    "jsonwebtoken": "8.5.0",
    "lodash": "4.17.11",
    "module-alias": "2.2.0",
    "next": "8.0.2",
    "nprogress": "0.2.0",
    "prisma-binding": "2.3.3",
    "prisma-client-lib": "1.27.0",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-apollo": "2.4.1",
    "react-dom": "16.8.3",
    "react-icons-kit": "^1.2.1",
    "react-transition-group": "2.5.3",
    "styled-components": "4.1.3",
    "styled-css-grid": "^1.2.1",
    "styled-normalize": "8.0.6",
    "styled-system": "^3.2.0",
    "uuid": "3.3.2",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.1.13",
    "@storybook/addon-info": "4.1.13",
    "@storybook/addon-knobs": "4.1.13",
    "@storybook/addon-notes": "4.1.13",
    "@storybook/addon-viewport": "4.1.13",
    "@storybook/react": "4.1.13",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.17.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "foreman": "3.0.1",
    "fs-extra": "^7.0.0",
    "husky": "1.3.1",
    "inquirer": "6.2.2",
    "jest": "24.1.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "puppeteer": "1.12.2",
    "react-test-renderer": "16.8.3"
  }
}
