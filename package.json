{
  "name": "inteligir",
  "description": "Share knowledge from the web through playlists",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/inteligir"
  },
  "scripts": {
    "codegen": "graphql-let",
    "dev": "env-cmd yarn codegen && next",
    "dev:prisma": "env-cmd prisma2 generate --watch",
    "dev:server": "env-cmd yarn codegen && next",
    "dev:db": "env-cmd prisma2 studio --experimental",
    "db:seed": "env-cmd ts-node prisma/seed",
    "db:introspect": "env-cmd prisma2 introspect",
    "db:migrate": "env-cmd prisma2 migrate --experimental",
    "build": "yarn codegen && next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "@prisma/client": "^2.0.0-preview022",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-link-schema": "^1.2.4",
    "apollo-server-micro": "^2.10.1",
    "apollo-utilities": "^1.3.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.3",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.3.2",
    "nexus": "0.12.0-rc.13",
    "nexus-prisma": "^0.10.0",
    "nprogress": "^0.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^4.10.1",
    "react-swipeable-views": "^0.13.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/plugin-helpers": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@graphql-codegen/typescript-react-apollo": "^1.12.2",
    "@graphql-codegen/typescript-resolvers": "^1.12.2",
    "@prisma/sdk": "^0.0.221",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "concurrently": "^5.1.0",
    "env-cmd": "^10.1.0",
    "graphql-let": "^0.9.0",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prisma2": "^2.0.0-preview022",
    "typescript": "^3.8.3"
  }
}
