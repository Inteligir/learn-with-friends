{
  "name": "inteligir",
  "description": "Making open source accessible for everyone",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inteligir/inteligir-platform"
  },
  "engines": {
    "node": "10.14.2",
    "npm": "6.4.1"
  },
  "scripts": {
    "setup": "npm i && node tools/generate_env && npm run deploy:schema",
    "dev": "env-cmd ./config/local.env nodemon server/index.js --ext js,graphql",
    "build": "env-cmd ./config/local.env next build client",
    "deploy": "env-cmd ./config/local.env node tools/deploy",
    "deploy:schema": "env-cmd ./config/local.env prisma deploy",
    "analyze": "source-map-explorer client/build/static/js/main.*",
    "test": "env-cmd ./config/test.env jest --watch --runInBand",
    "heroku-postbuild": "npm run build"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "./server",
    "@client": "./client"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup.js"
  },
  "dependencies": {
    "adorable-avatars": "0.2.1",
    "apollo-boost": "0.1.23",
    "apollo-client": "2.4.8",
    "aws-sdk": "2.382.0",
    "bcrypt": "3.0.3",
    "boom": "7.3.0",
    "env-cmd": "8.0.2",
    "express-async-errors": "3.1.1",
    "graphql-shield": "5.1.0",
    "graphql-yoga": "1.17.0",
    "helmet": "3.15.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "4.17.11",
    "module-alias": "2.1.0",
    "next": "7.0.2",
    "nprogress": "0.2.0",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "polished": "2.3.1",
    "prisma-binding": "2.2.12",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-adopt": "0.6.0",
    "react-apollo": "2.3.3",
    "react-dom": "16.7.0",
    "react-transition-group": "2.5.2",
    "styled-components": "4.1.3",
    "uuid": "3.3.2",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "5.11.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.2",
    "foreman": "3.0.1",
    "husky": "1.3.1",
    "inquirer": "6.2.1",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "puppeteer": "1.11.0",
    "source-map-explorer": "1.6.0"
  }
}
